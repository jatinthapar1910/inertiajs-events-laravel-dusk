{
    "name": "protonemedia/inertiajs-events-laravel-dusk",
    "description": "Laravel package to invert Eloquent scopes.",
    "keywords": [
        "protonemedia",
        "inertiajs-events-laravel-dusk"
    ],
    "homepage": "https://github.com/protonemedia/inertiajs-events-laravel-dusk",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Pascal Baljet",
            "email": "pascal@protone.media",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
        "laravel/dusk": "^6.15"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "ProtoneMedia\\InertiaJsEventsLaravelDusk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProtoneMedia\\InertiaJsEventsLaravelDusk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
